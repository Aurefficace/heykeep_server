{% extends 'baseSecurity.html.twig' %}

{% block title %}Log in!
{% endblock %}
{% block stylesheets %}
	<link href="{{ asset('heykeep/css/login.css') }}" rel="stylesheet" type="text/css">
{% endblock %}
{% block body %}
	<div class="limiter">
		<form class="login100-form validate-form" method="post">
			<div class="wrap-input100 validate-input" data-validate="Valid email is required: ex@abc.xyz">
				<input type="text" name="email" id="inputEmail" class="input100" required autofocus>
				<span class="focus-input100"></span>
				<span class="symbol-input100">
					<i class="fa fa-envelope" aria-hidden="true"></i>
				</span>
			</div>
			<div class="container-login100-form-btn">
				<button class="login100-form-btn" type="submit">
					Envoie moi mon mot de passe Wesh
				</button>
			</div>

		</form>
	</div>
{% endblock %}
